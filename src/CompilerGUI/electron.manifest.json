{
  "executable": "CompilerGUI",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "CompilerGUI",
  "author": "Kevin Gunn",
  "copyright": "Copyright � 2023",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.CompilerGUI.app",
    "productName": "CompilerGUI",
    "copyright": "Copyright © 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "bin/build/icon.ico",
      "extraResources": [
        {
          "from": "./bin",
          "to": "bin",
          "filter": [ "**/*" ]
        }
      ],
      "files": [
        {
          "from": "./ElectronHostHook/node_modules",
          "to": "ElectronHostHook/node_modules",
          "filter": [ "**/*" ]
        },
        "**/*"
      ]
    },
    "mac": {
      "icon": "bin/build/icon.png",
      "hardenedRuntime": true,
      "entitlements": "../../../build/entitlements.mac.inherit.plist",
      "target": [ "dmg", "zip" ],
      "extraResources": [
        {
          "from": "./bin",
          "to": "bin",
          "filter": [ "**/*" ]
        }
      ],
      "files": [
        {
          "from": "./ElectronHostHook/node_modules",
          "to": "ElectronHostHook/node_modules",
          "filter": [ "**/*" ]
        },
        "**/*"
      ]
    },
    "linux": {
      "icon": "bin/build/256x256.png",
      "extraResources": [
        {
          "from": "./bin",
          "to": "bin",
          "filter": [ "**/*" ]
        }
      ],
      "files": [
        {
          "from": "./ElectronHostHook/node_modules",
          "to": "ElectronHostHook/node_modules",
          "filter": [ "**/*" ]
        },
        "**/*"
      ]
    }
  }
}
